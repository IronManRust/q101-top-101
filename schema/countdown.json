{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "title": {
            "type": "string"
        },
        "years": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "year": {
                        "type": "integer"
                    },
                    "items": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "position": {
                                    "type": "integer"
                                },
                                "artist": {
                                    "type": "string"
                                },
                                "facebook": {
                                    "type": "string"
                                },
                                "twitter": {
                                    "type": "string"
                                },
                                "song": {
                                    "type": "string"
                                },
                                "musicVideoURL": {
                                    "type": "string"
                                },
                                "musicVideoSource": {
                                    "type": "string",
                                    "enum": [
                                        "none",
                                        "general",
                                        "youtube",
                                        "vimeo",
                                        "dailymotion"
                                    ]
                                }
                            },
                            "additionalProperties": false,
                            "required": [
                                "position",
                                "artist",
                                "song"
                            ]
                        }
                    }
                },
                "additionalProperties": false,
                "required": [
                    "year",
                    "items"
                ]
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "title",
        "years"
    ]
}